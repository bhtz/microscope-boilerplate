{
  "Logging": {
    "LogLevel": {
      "Default": "Error",
      "Microsoft.AspNetCore": "Error"
    }
  },

  "Tenants": [
    {
      "Authority": "http://host.docker.internal:8083/realms/master",
      "Audience": "boilerplate"
    }
  ],

  "MasterKey": "http://localhost:8083/realms/master_devmasterkey",

  "Users": {
    "Adapter": "keycloak",
    "UserServiceEndpoint": "http://host.docker.internal:8083/admin/realms/master/users"
  },

  "Mail": {
    "Adapter": "smtp"
  },

  "Storage": {
    "Adapter": "filesystem",
    "Host": "Uploads"
  },

  "Bus": {
    "Adapter": "rabbitmq",
    "Host": "rabbitmq://host.docker.internal",
    "Username" : "guest",
    "Password" : "guest"
  },

  "EnableMigration": "true",
  "DatabaseProvider" : "postgres",
  "ConnectionStrings": {
    "TodoAppService": "User ID =postgres;Password=root;Server=host.docker.internal;Port=5432;Database=postgres;Integrated Security=true;Pooling=true;"
  },

  "FeatureManagement": {
    "sampleFeatureFlagOne": true,
    "sampleFeatureFlagTwo": false
  }
}
